<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PracticaAPI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PracticaAPI"
    xmlns:views="clr-namespace:RawPostres.Views"
    Title="PracticaAPI">

    <ShellContent
        Title="Inicio"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Postres"
        ContentTemplate="{DataTemplate views:PostreListPage}"
        Route="PostreListPage" />

</Shell>
